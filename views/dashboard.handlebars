        {{> individualPosts post}}
        {{#each comments as |comment|}}            
        {{/each}}
        {{!-- <button class="edit-post-button" data-id="{{post.id}}">Edit me</button> --}}
        <button class="delete-something-button" data-id="{{post.id}}">Delete me</button>
    {{/each}}
</div>

<button class="create-something-button" type="submit"><a href="/dashboard/createPost">Create New Post</a></button>

