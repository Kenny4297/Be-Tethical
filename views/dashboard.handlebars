<h1>Dashboard page</h1>

{{#if posts.length}}
<div>
    <h3>Your Current Posts</h3>
    {{#each posts as |post|}}
        {{> individualPosts post}}
    {{/each}}
</div>
{{/if}}

<button type="submit"><a href="/dashboard/createPost">Create New Post</a></button>

<script src="/javascript/addPost.js"></script>